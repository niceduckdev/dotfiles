super + t
	alacritty

super + @space
	rofi -show drun

super + q
	bspc node -c

super + v
	bspc node focused -t \~floating

super + f
	 bspc node -t \~fullscreen 

super + {ampersand,eacute,quotedbl,apostrophe,parenleft,section,egrave,exclam}
	bspc desktop -f {one,two,three,four,five,six,seven,eight}

super + shift + {ampersand,eacute,quotedbl,apostrophe,parenleft,section,egrave,exclam}
	bspc node -d {one,two,three,four,five,six,seven,eight} --follow

super + {h,j,k,l}
	bspc node -f {west,south,north,east}

super + shift + {h,j,k,l}
	bspc node -z {left -70 0, bottom 0 70, top 0 -70, right 70 0}

super + alt + {h,j,k,l}
	bspc node -s {west,south,north,east}

XF86AudioLowerVolume
    amixer set Master 10%- unmute
XF86AudioRaiseVolume
    amixer set Master 10%+ unmute
XF86AudioMute
    amixer set Master toggle

super + s
	rofi -modi 'semesters:setup-for-college semesters' -show semesters -p '' -theme-str 'prompt \{ enabled: false; \}'

super + c
	rofi -modi 'courses:setup-for-college courses' -show courses -p '' -theme-str 'prompt \{ enabled: false; \}'

super + p
	alacritty --working-directory /home/kaj/school/course

XF86MonBrightness{Up,Down}  
	brightnessctl s 25{+,-}

Print
	maim -s | xclip -selection clipboard -t image/png
